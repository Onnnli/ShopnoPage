        title Shopno one page
    body
        header#header
            .container
                .navigation
                    a.logo(href="#header")
                        p.logoText sh
                            span o
                            | pno
                    .burger
                        .burgerLine
                            span
                        .navigationMenu
                            a.navigationMenu__link(href="#about") ABOUT US
                            a.navigationMenu__link(href="#") featured works
                            a.navigationMenu__link(href="#") our services
                            a.navigationMenu__link(href="#") small team
                            a.navigationMenu__link(href="#") what they says
                            a.navigationMenu__link(href="#") contacts
                .headerBlock
                    h1.titleSection Say Hello to 
                        span &nbsp;SHOPNO!&nbsp;
                    h1.titleSection 
                        span &nbsp;Agency 
                        | &nbsp;CORPORATE SHOPNO Theme

                    // modal

                    button.btn.btn-outline-dark.headerBtn.font-weight-bold(type='button' data-toggle='modal' data-target='#exampleModal' data-whatever='@getbootstrap') CONTACT US
                    #exampleModal.modal.fade(tabindex='-1' role='dialog' aria-labelledby='exampleModalLabel' aria-hidden='true')
                        .modal-dialog(role='document')
                            .modal-content
                                .modal-header
                                    h5#exampleModalLabel.modal-title New message
                                    button.close(type='button' data-dismiss='modal' aria-label='Close')
                                        span(aria-hidden='true') ×
                                .modal-body
                                    form
                                        .form-group
                                            label.col-form-label(for='recipient-name') Recipient:
                                            input#recipient-name.form-control(type='text' placeholder='SHOPNO')
                                        .form-group
                                            label.col-form-label(for='message-text') Message:
                                            textarea#message-text.form-control(placeholder="MESSAGE")
                                .modal-footer
                                    button.btn.btn-secondary(type='button' data-dismiss='modal') Close
                                    button.btn.btn-primary(type='button') Send message
                .juice
                    img(src="img/juice.png", alt="sok")

